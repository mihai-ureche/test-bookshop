{
  "name": "test-bookshop",
  "version": "0.1.0",
  "description": "Test bookshop integration",
  "homepage": "https://github.com/mihai-ureche/test-bookshop#readme",
  "bugs": {
    "url": "https://github.com/mihai-ureche/test-bookshop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mihai-ureche/test-bookshop.git"
  },
  "license": "ISC",
  "author": "Mihai Ureche",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "bookshop:watch": "bookshop-browser",
    "bookshop-sass:build": "bookshop-sass -b component-library -o site/assets/bookshop.css",
    "bookshop-sass:watch": "bookshop-sass -b component-library -o site/assets/bookshop.css -w",
    "eleventy:build": "eleventy",
    "eleventy:watch": "ELEVENTY_ENV=development eleventy --serve",
    "start": "npm-run-all bookshop-sass:build --parallel bookshop-sass:watch bookshop:watch eleventy:watch",
    "bookshop:generate": "npm run eleventy:build && npx @bookshop/generate"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@bookshop/browser": "^3.16.5",
    "@bookshop/eleventy-bookshop": "^3.16.5",
    "@bookshop/eleventy-engine": "^3.16.5",
    "@bookshop/generate": "^3.16.5",
    "@bookshop/sass": "^3.16.5",
    "npm-run-all": "^4.1.5"
  }
}
